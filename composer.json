{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "App\\": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AppBundle\\": "tests/AppBundle",
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.34",
        "symfony/console": "^4.0",
        "symfony/dotenv": "*",
        "symfony/framework-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/dom-crawler": "*",
        "symfony/flex": "^1.19",
        "symfony/translation": "4.0.*",
        "symfony/form": "4.0.*",
        "symfony/validator": "4.0.*",
        "symfony/security-csrf": "4.0.*",
        "symfony/twig-bundle": "^4.0",
        "symfony/asset": "4.0.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0"

       
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.0.*"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
